<h1>RssEntries#index</h1>
<table class="table">
   <thead>
      <tr>
         <th>Title</th>
         <th>Post DateTime</th>
         <th></th>
         <th></th>
      </tr>
   </thead>
   <tbody>
      <% @rails_entries.each do |item| %>
         <tr>
            <td><%= item.title %></td>
            <td><%= item.entry_updated_at %></td>
            <td><button type="button" class="btn btn-primary">Detail</button></td>
            <td><button type="button" class="btn btn-danger">Read</button></td>
         </tr>
      <% end %>
   </tbody>
</table>
